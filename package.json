{
  "name": "vaizbtgads",
  "version": "1.0.0",
  "description": "Vaizbtgads - Content Blocker untuk Browser yang efisien",
  "type": "module",
  "scripts": {
    "build": "node tools/build.js",
    "validate": "node tools/validate.js",
    "create-firefox-zip": "node tools/create-firefox-zip-manual.js",
    "prepare-submission": "node tools/prepare-submission.js",
    "lint": "eslint --no-warn-ignored --ignore-pattern \"**/lib/\" --ignore-pattern \"**/npm/\" -- \"./src/js/*.js\" \"./src/js/**/*.js\" \"./**/*.json\" \"./platform/**/*.js\"",
    "test": "npm run validate && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XTEID/vaizbtgads.git"
  },
  "keywords": [
