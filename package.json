  "author": "Developer",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/XTEID/vaizbtgads/issues"
  },
  "homepage": "https://github.com/XTEID/vaizbtgads#readme",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@eslint/json": "^0.13.1",
    "eslint": "^9.34.0",
    "eslint-formatter-compact": "^8.40.0",
    "globals": "^15.14.0"
  }
